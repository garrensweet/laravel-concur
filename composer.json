{
  "name": "vdpoel/concur-sdk-php",
  "description": "A PHP SDK for interfacing with the Concur API",
  "version": "0.0.2",
  "license": "MIT",
  "authors": [
    {
      "name": "Garren Sweet",
      "email": "garren@vandepoelproductions.com"
    },
    {
      "name": "Brian Lee",
      "email": "brian@vandepoelproductions.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "ext-xml": "*",
    "laravel/framework": "^5.6.0",
    "guzzlehttp/guzzle": "^6.3@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "orchestra/testbench": "^3.7.0"
  },
  "autoload": {
    "psr-4": {
      "VdPoel\\Concur\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VdPoel\\Concur\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VdPoel\\Concur\\ConcurServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
